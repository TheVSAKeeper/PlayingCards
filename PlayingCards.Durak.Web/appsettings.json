{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SignalR": {
    "TransportType": "ServerSentEventsOnly",
    "AllowPollingFallback": true,
    "MaxConnections": 1000,
    "ConnectionTimeoutSeconds": 60,
    "KeepAliveIntervalSeconds": 15,
    "MaxMessageSize": 32768,
    "StreamBufferCapacity": 10,
    "EnableDetailedErrors": false,
    "ReconnectDelays": [
      0,
      2000,
      10000,
      30000
    ],
    "MaxReconnectAttempts": 10,
    "EnableLogging": true,
    "LogLevel": 2,
    "HubPath": "/gameHub",
    "EnableCors": false,
    "CorsOrigins": []
  }
}
